{
  "name": "react-native-tailwind-nextjs",
  "version": "0.0.0-locked",
  "author": "nongdan.dev",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "lint": "pnpm -s normalize && pnpm -s eslint && pnpm -s stylelint && pnpm -s prettier",
    "normalize": "node ./devtools normalize",
    "eslint": "node ./devtools eslint",
    "stylelint": "node ./devtools stylelint",
    "prettier": "node ./devtools prettier",
    "ts": "node ./devtools ts"
  },
  "engines": {
    "node": ">=24.11",
    "pnpm": ">=10"
  },
  "dependencies": {
    "json5": "2.2.3",
    "tsconfig-paths": "4.2.0"
  },
  "devDependencies": {
    "@types/node": "25.0.9",
    "typescript": "5.9.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "eslint-plugin-no-relative-import-paths@1.6.1": "patches/eslint-plugin-no-relative-import-paths@1.6.1.patch",
      "react-native-web": "patches/react-native-web.patch",
      "eslint-plugin-import": "patches/eslint-plugin-import.patch"
    }
  }
}
